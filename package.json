{
  "name": "portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm run dev-frontend\" \"npm run start-backend\"",
    "watch": "vite",
    "dev-frontend": "vite --config vite.config.js",
    "start-backend": "node backend/index.js",
    "build": "npm run build-frontend && npm run build-backend",
    "build-frontend": "vite build --config vite.config.js",
    "build-backend": "echo \"No backend build script yet\"",
    "lint": "eslint . --fix",
    "preview": "vite preview --config vite.config.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "prettier-check": "prettier --check ."
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "cors": "^2.8.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.9.0",
    "@eslint/js": "^9.9.0",
    "@storybook/addon-essentials": "^8.1.11",
    "@storybook/addon-interactions": "^8.1.11",
    "@storybook/addon-links": "^8.1.11",
    "@storybook/addon-onboarding": "^8.1.11",
    "@storybook/blocks": "^8.1.11",
    "@storybook/react": "^8.3.3",
    "@storybook/react-vite": "^8.3.3",
    "@storybook/test": "^8.3.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^9.0.1",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "eslint-plugin-storybook": "^0.8.0",
    "globals": "^15.9.0",
    "storybook": "^8.3.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "unocss": "^0.62.4",
    "vite": "^5.4.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
